<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.usuario.MeusDados">


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        android:orientation="vertical">

        <TextView android:id="@+id/txtTitulo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/campos"
            android:text="@string/meus_dados"
            android:enabled="false"
            android:gravity="center"
            android:textColor="@color/colorPrimary"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/campos"
            android:gravity="center"
            android:layout_marginTop="16dp"
            android:text="@string/txtFragmenteNomeMyDB"

            android:textColor="@color/colorAccent" />

        <EditText android:id="@+id/editPrimeiroNomeMydb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@color/campos"
            android:gravity="left"
            android:enabled="false"
            android:textColor="@color/colorPrimary"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/campos"
            android:gravity="center"
            android:layout_marginTop="16dp"
            android:text="@string/txtFragmentSobreNomeMyDB"
            android:textColor="@color/colorPrimaryDark" />

        <EditText android:id="@+id/editSobreNomeMydb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@color/campos"
            android:gravity="left"
            android:enabled="false"
            android:inputType="phone"
            android:textColor="@color/colorPrimary"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/textEmail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/campos"
            android:gravity="center"
            android:layout_marginTop="16dp"
            android:text="@string/txtFragmenteEmailMyDB"
            android:textColor="@color/colorAccent" />
        <EditText android:id="@+id/editEmailMydb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@color/campos"
            android:inputType="textEmailAddress"
            android:gravity="left"
            android:enabled="false"
            android:textColor="@color/colorPrimary"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/textSenha"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/campos"
            android:gravity="center"
            android:layout_marginTop="16dp"
            android:text="@string/txtFragmentSenhaMyDB"
            android:textColor="@color/colorAccent" />


        <EditText android:id="@+id/editSenhaMydb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:enabled="false"
            android:background="@color/campos"
            android:gravity="left"
            android:inputType="textPassword"
            android:textColor="@color/colorPrimary"
            android:textSize="18sp"
            android:textStyle="bold" />




        <CheckBox android:id="@+id/checkBoxTermoMydb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@color/campos"
            android:enabled="false"
            android:gravity="left"
            android:text="@string/chFragmenteTermoMyDB"
            android:textColor="@color/colorPrimary"
            android:textSize="18sp"
            android:textStyle="bold" />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">



        </androidx.appcompat.widget.LinearLayoutCompat>


    </androidx.appcompat.widget.LinearLayoutCompat>

</FrameLayout>
